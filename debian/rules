#!/usr/bin/make -f
# -*- makefile -*-
# Sample debian/rules that uses debhelper.
# This file was originally written by Joey Hess and Craig Small.
# As a special exception, when this file is copied by dh-make into a
# dh-make output file, you may use that output file without restriction.
# This special exception was added by Craig Small in version 0.37 of dh-make.

# Uncomment this to turn on verbose mode.
#export DH_VERBOSE=1

%:
	dh  $@

override_dh_install:
	dh_install
	dh_link /etc/network/if-pre-up.d/pan /etc/network/if-post-down.d/pan
	dh_link /etc/network/if-up.d/addresses /etc/network/if-down.d/addresses
	dh_link /etc/network/if-pre-up.d/conflicts /etc/network/if-post-down.d/conflicts
	dh_link /etc/network/if-pre-up.d/vpnc /etc/network/if-down.d/vpnc

